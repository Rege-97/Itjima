<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itjima_server.mapper.TransactionMapper">
  <insert id="insert" parameterType="com.itjima_server.domain.transaction.Transaction"
    useGeneratedKeys="true" keyProperty="id">
    INSERT INTO TRANSACTIONS (agreement_id, type, amount, status)
    VALUES (#{agreementId}, #{type}, #{amount}, #{status})
  </insert>
</mapper>